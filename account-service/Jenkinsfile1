pipeline{
  any agent
  stages {
    stage('git') {
      steps {
        git 'https://github.com/mspk1/sample-spring-microservices.git'
        dir("account-service")
          sh "mvn clean install"
          }
      }
      
    stage('build image') {
      steps {
        dir ('account-service'){
        sh 'docker build -t javaimage .'
       }
     }
   }  
   }  
   
}   
