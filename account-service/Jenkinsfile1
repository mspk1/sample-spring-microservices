pipeline{
  node ('new')
  stages {
    stage('maven') {
      steps {
        dir ("account-service")
        sh "mvn clean install package"
        }
     }   
      
    stage('build image') {
      steps {
        dir ('account-service'){
        sh 'docker build -t javaimage .'
       }
     }
   }  
   }  
   
}   
